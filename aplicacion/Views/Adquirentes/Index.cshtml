@model IEnumerable<aplicacion.models.Adquirente>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RunRut)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PorcentajeDerecho)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PorcentajeDerechoNoAcreditado)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NumAtencionNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RunRut)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PorcentajeDerecho)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PorcentajeDerechoNoAcreditado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NumAtencionNavigation.NumAtencion)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdAdquirente">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdAdquirente">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdAdquirente">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
